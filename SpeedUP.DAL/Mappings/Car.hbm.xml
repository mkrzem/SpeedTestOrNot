<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SpeedUp"
                   namespace="SpeedUp.Domain">
  <class name="Car">
    <id name="Id">
      <generator class="int"/>
    </id>
    <property name="Make"/>
    <property name="Model"/>
    <property name="Year"/>
  </class>
</hibernate-mapping>