<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SpeedUP.DAL"
                   namespace="SpeedUP.DAL.Domain">
  <class name="Car" table="CAR" lazy="false">
    <id name="Id">
      <generator class="increment"/>
    </id>
    <property name="Make"/>
    <property name="Model"/>
    <property name="Year"/>
  </class>
</hibernate-mapping>